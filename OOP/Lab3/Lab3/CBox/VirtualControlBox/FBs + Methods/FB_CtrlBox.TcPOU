    <Method Name="ModeSelection" Id="{3f8742b6-e972-45ff-a32a-b2abdc82c2a2}">
      <Declaration><![CDATA[METHOD ModeSelection : BOOL
VAR_INPUT
	bInOff : BOOL;
	bInAuto : BOOL;
	bInManual : BOOL; 
END_VAR
VAR_OUTPUT
	bOutOff : BOOL;
	bOutAuto : BOOL;
	bOutManual : BOOL; 
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[
IF NOT bInOff AND NOT bInAuto THEN
	bOutManual := TRUE;
ELSIF NOT bInOff AND NOT bInManual THEN
	bOutAuto := TRUE; 
ELSE
	bInOff := TRUE;
END_IF
		

]]></ST>
      </Implementation>

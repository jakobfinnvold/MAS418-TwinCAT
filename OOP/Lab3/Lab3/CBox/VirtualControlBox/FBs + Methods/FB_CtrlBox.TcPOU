    <Method Name="CheckStatus" Id="{d76ca786-9612-4368-9a84-956521259606}">
      <Declaration><![CDATA[METHOD CheckStatus : BOOL
VAR_INPUT
	bOff : BOOL;
	bAuto : BOOL;
	bManual : BOOL; 
	bStartButton : BOOL;
	bStopButton : BOOL; 
END_VAR
VAR_OUTPUT
	bNotReady : BOOL; 
	bReady : BOOL; 
	bStarting : BOOL;
	bRunning : BOOL; 
	bStopping : BOOL; 
	bError : BOOL; 
	
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF bOff THEN
	bNotReady := TRUE; 
END_IF

IF bAuto OR bManual AND NOT bStartButton THEN
	bReady := TRUE; 
END_IF

IF bStartButton THEN
	bStarting := TRUE;
END_IF

CASE bStarting OF
	
END_CASE]]></ST>
      </Implementation>
